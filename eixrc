# STRING
# The file which is used instead of /etc/eixrc and ~/.eixrc.
# This variable can of course only be set in the environment.
EIXRC=""

# BOOLEAN
# This variable is only used for delayed substitution.
# It defines whether you have a wide terminal (>80 columns).
# An empty value or "auto" means that COLUMNS is used/calculated.
WIDETERM=""

# INTEGER
# Set the terminal width to define the WIDETERM expansion.
# An empty value or "auto" means that a heuristic is used instead.
COLUMNS=""

# PREFIXSTRING
# This path is prepended to source commands in /etc/eixrc.
# If set in /etc/eixrc it temporarily overrides the environment.
# You must not use delayed substitution in this variable.
EIXRC_SOURCE=""

# STRING
# This variable is used for delayed substitution in EIX_SYNC_CONF.
# It contains code which is evaluated by eix-sync, so be aware of security!
EIX_SYNC_OPTS=""

# STRING
# The content of this variable is appended to /etc/eix-sync.conf
# In particular, it can be used to override options set in that file.
# Parts of this variable are evaluated in eix-sync: Be aware of security!
EIX_SYNC_CONF="%{EIX_SYNC_OPTS}"

# STRING
# This variable contains default options for the eix-remote script.
# Note that its content is evaluated, so quote correctly. Typical example:
# EIX_REMOTE_OPTS='-f /var/lib/layman/eix-caches.tar.bz2'
EIX_REMOTE_OPTS=""

# STRING
# This variable contains default options for the eix-layman script.
# Note that its content is evaluated, so quote correctly.
EIX_LAYMAN_OPTS=""

# STRING
# This variable contains default options for the eix-test-obsolete script.
EIX_TEST_OBSOLETE_OPTS="-d"

# STRING
# This variable contains default arguments for the eix-installed-after script.
EIX_INSTALLED_AFTER=""

# PREFIXSTRING
# If this variable is set in the environment, then it is prefixed
# to the path where /etc/eixrc is searched. If it is not set in the
# environment, then PORTAGE_CONFIGROOT is used instead.
# If both are unset, the EIX_PREFIX default value is used instead.
# Moreover, EIX_PREFIX is used for delayed substitution for EPREFIX
# and EPREFIX_SOURCE.
EIX_PREFIX=""

# PREFIXSTRING
# This variable is only used for delayed substitution.
# It influences most paths except for $HOME/.eixrc, the cache file
# passed in the command line, PORTAGE_PROFILE, PORTDIR,
# and overlays. In contrast to EPREFIX, further exceptions are:
# PORTAGE_CONFIGROOT, portage/scripts-internal stuff and the eix cachefile.
ROOT=""

# PREFIXSTRING
# This variable is used for delayed substitution for path prefixes.
# It influences most paths except for $HOME/.eixrc, the cache file
# passed in the command line, PORTAGE_PROFILE, PORTDIR, and overlays.
EPREFIX="%{EIX_PREFIX}"

# PREFIXSTRING
# This variable is only used for delayed substitution.
# It is the path prepended to PORTAGE_PROFILE, PORTDIR, and overlays.
EPREFIX_TREE=""

# PREFIXSTRING
# It applies for those paths for which EPREFIX and ROOT should both apply.
# So you can decide here what to do if both are nonempty. For instance,
# the choice %{EPREFIX}%{ROOT} will apply both; the default applies EPREFIX
# but not ROOT for these paths in such a case (i.e. if both are nonempty).
EPREFIX_ROOT="%{??EPREFIX}%{EPREFIX}%{else}%{ROOT}%{}"

# PREFIXSTRING
# This path is prepended to the /etc paths.
PORTAGE_CONFIGROOT="%{EPREFIX}"

# PREFIXSTRING
# This variable is only used for delayed substitution.
# It is the path prepended to MAKE_GLOBALS and PORTAGE_REPOS_CONF.
PORTAGE_DEFAULTS="%{EPREFIX}/usr/share/portage/config"

# STRING
# This file is used instead of %{PORTAGE_CONFIGROOT}/etc/make.globals
# if it exists. This is reasonable for >=portage-2.2*
MAKE_GLOBALS="%{PORTAGE_DEFAULTS}/make.globals"

# STRING
# This file is used to set the default repository information.
PORTAGE_REPOS_CONF="%{PORTAGE_DEFAULTS}/repos.conf"

# PREFIXSTRING
# This variable is only used for delayed substitution.
# It is used as prefix in the EXEC_EBUILD* and EBUILD_DEPEND_TEMP defaults.
EPREFIX_PORTAGE_EXEC="%{EPREFIX}"

# PREFIXSTRING
# This path is prepended to source commands in /etc/make.{conf,globals}.
EPREFIX_SOURCE="%{EIX_PREFIX}"

# PREFIXSTRING
# Prefix to the path where eix expects information about installed packages.
EPREFIX_INSTALLED="%{EPREFIX_ROOT}"

# PREFIXSTRING
# This prefix is prepended to the portage cache.
EPREFIX_PORTAGE_CACHE="%{EPREFIX}"

# PREFIXSTRING
# This prefix is prepended to overlays when their files are accessed.
EPREFIX_ACCESS_OVERLAYS=""

# PREFIXSTRING
# This path is prepended to PORTDIR.
EPREFIX_PORTDIR="%{EPREFIX_TREE}"

# PREFIXSTRING
# This path is prepended to PORTIDIR_OVERLAY values.
EPREFIX_OVERLAYS="%{EPREFIX_TREE}"

# PREFIXSTRING
# This path is prepended to PORTAGE_PROFILE.
EPREFIX_PORTAGE_PROFILE="%{EPREFIX_TREE}"

# PREFIXSTRING
# This is prepended to overlays in eix database to test whether they exist.
EPREFIX_VIRTUAL="%{EPREFIX_TREE}"

# STRING
# This file is the default eix cache.
EIX_CACHEFILE="%{EPREFIX}/var/cache/eix/portage.eix"

# STRING
# This file is the previous eix cache (used by eix-diff and eix-sync).
EIX_PREVIOUS="%{EPREFIX}/var/cache/eix/previous.eix"

# STRING
# This is the eix cache used when -R is in effect. If the string is nonempty,
# eix-remote uses this file for the output.
EIX_REMOTE1="%{EPREFIX}/var/cache/eix/remote.eix"

# STRING
# This is the eix cache used when -Z is in effect. If the string is nonempty,
# eix-remote uses this file for the output.
EIX_REMOTE2="%{EPREFIX}/var/cache/eix/remote2.eix"

# INTEGER
# 1, 2, or 0 means that eix option -R, -Z, or neither is the default.
REMOTE_DEFAULT="0"

# STRING
# This is a local copy of the remote archive used by eix-remote.
# If the name is empty, only a temporary file is used.
EIX_REMOTEARCHIVE1="%{EPREFIX}/var/cache/eix/remote.tar.bz2"

# STRING
# This is a local copy of the remote archive used by eix-remote.
# If the name is empty, only a temporary file is used.
EIX_REMOTEARCHIVE2="%{EPREFIX}/var/cache/eix/remote2.tar.bz2"

# STRING
# If nonempty, this will override the heuristics for eix-remote -L.
LOCAL_LAYMAN=""

# STRING
# The path to the ebuild executable.
EXEC_EBUILD="%{EPREFIX_PORTAGE_EXEC}/usr/bin/ebuild"

# STRING
# The path to the ebuild.sh executable.
EXEC_EBUILD_SH="%{EPREFIX_PORTAGE_EXEC}/usr/lib64/portage/bin/ebuild.sh"

# STRING
# The path to the tempfile generated by "ebuild depend".
EBUILD_DEPEND_TEMP="%{EPREFIX_PORTAGE_EXEC}/var/cache/edb/dep/aux_db_key_temp"

# STRING
# This file is considered as the world file.
EIX_WORLD="%{EPREFIX_ROOT}/var/lib/portage/world"

# STRING
# This file is considered as the world_sets file.
EIX_WORLD_SETS="%{EIX_WORLD}_sets"

# BOOLEAN
# Store the information of the world file in the cache file.
# Set this to false if you do not want that everybody can get this information.
SAVE_WORLD="true"

# BOOLEAN
# Prefer the current world file (if readable) over the data in the cachefile.
CURRENT_WORLD="true"

# STRING
# Attempt to change to this user if possible. See EIX_UID.
EIX_USER="portage"

# STRING
# Attempt to change to this group if possible. See EIX_GID.
EIX_GROUP="%{EIX_USER}"

# INTEGER
# If EIX_USER is empty or nonexistent, use this user id.
# In this case and if ${EIX_UID} <= 0, the user id is not changed.
EIX_UID="250"

# INTEGER
# If EIX_GROUP is empty or nonexistent, use this group id.
# In this case and if ${EIX_GID} <= 0, the group id is not changed.
EIX_GID="%{EIX_UID}"

# STRING
# This variable is passed unchanged to ebuild.sh
# Usually ebuild.sh uses it to calculate the PATH.
PORTAGE_ROOTPATH="/opt/bin:/usr/x86_64-pc-linux-gnu/gcc-bin/4.7.3"

# STRING
# This variable is passed unchanged to ebuild.sh
# Usually ebuild.sh uses it to calculate the PATH.
PORTAGE_BIN_PATH=""

# STRING
# The default ARCH if none is specified by the profile.
DEFAULT_ARCH="amd64"

# INTEGER
# This value is used as exit status if there are 0 matches.
# The value of COUNT_ONLY_PRINTED is honoured.
NOFOUND_STATUS="1"

# INTEGER
# This value is used as exit status if there are 2 or more matches.
# The value of COUNT_ONLY_PRINTED is honoured.
MOREFOUND_STATUS="0"

# INTEGER
# The maximal number of matches shown on terminal in non-compact mode.
# The value 0 means all matches are shown.
EIX_LIMIT="50"

# INTEGER
# The maximal number of matches shown on terminal in compact mode.
# The value 0 means all matches are shown.
EIX_LIMIT_COMPACT="200"

# BOOLEAN
# Whether --quick is on by default.
QUICKMODE="false"

# BOOLEAN
# Whether --care is on.
CAREMODE="false"

# BOOLEAN
# If true, use build time from BUILD_TIME entry instead of reading the install
# time from the directory timestamp. This is usually preferable but slower.
# The BUILD_TIME exists only for packages emerged with >=portage-2.2_rc63
USE_BUILD_TIME="true"

# BOOLEAN
# Whether --quiet is on by default.
QUIETMODE="false"

# STRING
# This string is appended to the output of --print.
# To read variables in a shell without omitting trailing spaces, use e.g.
# VAR=`PRINT_APPEND=x eix --print VAR` ; VAR=${VAR%x}
PRINT_APPEND="\\n"

# BOOLEAN
# If true, eix-diff will only consider version changes for installed packages.
DIFF_ONLY_INSTALLED="false"

# BOOLEAN
# If true, eix-diff will not consider slots for version changes.
DIFF_NO_SLOTS="false"

# BOOLEAN
# If false, eix-diff will mix deleted and changed packages
DIFF_SEPARATE_DELETED="true"

# BOOLEAN
# Should eix-diff print a header info line?
DIFF_PRINT_HEADER="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If false, RESTRICT and PROPERTIES values are output.
NO_RESTRICTIONS="false"

# BOOLEAN
# This variable is only used for delayed substitution.
# If false, tags are output for packages/versions with *.tbz2 files.
NO_BINARY="false"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, USE_EXPAND variables are output separately.
EIX_USE_EXPAND="true"

# BOOLEAN
# If true, calculate RESTRICT/PROPERTIES for installed versions.
RESTRICT_INSTALLED="true"

# BOOLEAN
# If true, read RESTRICT for installed versions always from disk.
# This is ignored if RESTRICT_INSTALLED=false.
CARE_RESTRICT_INSTALLED="true"

# BOOLEAN
# If true, read *DEPEND for installed versions always from disk.
# This is ignored if DEP=false.
DEPS_INSTALLED="true"

# BOOLEAN
# If true, store/use {R,P,H,}DEPEND (e.g. shown with eix -lv).
# Usage of DEP roughly doubles disk resp. memory requirements.
DEP="true"

# STRING
# Defines whether --compact or --verbose is on by default.
DEFAULT_FORMAT="normal"

# STRING
# Allowed values are true/false/never.
# If true, always print the number of matches (even 0 or 1) in the last line.
PRINT_COUNT_ALWAYS="false"

# BOOLEAN
# Do not output colors.
NOCOLORS="%{NOCOLOR}"

# BOOLEAN
# Do not output status line.
NOSTATUSLINE="%{NOCOLOR}"

# BOOLEAN
# Do not output percentage progress.
NOPERCENTAGE="%{NOCOLOR}"

# BOOLEAN
# This variable is only used for delayed substitution.
# It is the default for FORCE_{COLORS,STATUSLINE,PERCENTAGE}.
FORCE_USECOLORS="false"

# BOOLEAN
# Output colors even if not printing to a terminal.
FORCE_COLORS="%{FORCE_USECOLORS}"

# BOOLEAN
# Output status line even if not printing to a terminal.
FORCE_STATUSLINE="%{FORCE_USECOLORS}"

# BOOLEAN
# Output percentage progress even if not printing to a terminal.
FORCE_PERCENTAGE="%{FORCE_USECOLORS}"

# BOOLEAN
# This variable is only used for delayed substitution.
# If false, versions are only colored according to the local setting.
COLOR_ORIGINAL="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If false, COLOR_ORIGINAL=false has no effect on versions which are
# only locally masked (i.e. [m]).
COLOR_LOCAL_MASK="false"

# BOOLEAN
# Defines whether --versionsorted is on by default.
STYLE_VERSION_SORTED="false"

# BOOLEAN
# Defines whether --versionlines is on by default.
STYLE_VERSION_LINES="false"

# BOOLEAN
# This variable is only used for delayed substitution.
# If false, the slotnames appended to versions are not colored.
COLORED_SLOTS="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, separated slots from versions with a colon instead of braces.
COLON_SLOTS="false"

# BOOLEAN
# Whether default concatenation of queries is -o (or) or -a (and)
DEFAULT_IS_OR="false"

# BOOLEAN
# Whether checks for duplicate packages occur only among overlays
DUP_PACKAGES_ONLY_OVERLAYS="false"

# BOOLEAN
# Whether checks for duplicate versions occur only among overlays
DUP_VERSIONS_ONLY_OVERLAYS="false"

# STRING
# Which overlays to list (all/all-if-used/all-used/all-used-renumbered/no)
OVERLAYS_LIST="all-used-renumbered"

# INTEGER
# The default maximal levensthein distance for which a string is
# considered a match for the fuzzy match algorithm.
LEVENSHTEIN_DISTANCE="2"

# BOOLEAN
# Whether eix-update -v is on by default (output cache method per ebuild)
UPDATE_VERBOSE="false"

# STRING
# This string is appended to all cache methods using parse[*] or ebuild[*].
CACHE_METHOD_PARSE="#metadata-md5#metadata-flat#assign"

# STRING
# Portage cache-backend that should be used for PORTDIR
# (metadata[:*]/sqlite/flat[:*]/portage-2.1/parse[*][|]ebuild[*]/eix[*][:*])
PORTDIR_CACHE_METHOD="metadata-md5-or-flat"

# STRING
# Portage cache-backend that should be used for the overlays.
# (metadata[:*]/sqlite/flat[:*]/portage-2.1/parse[*][|]ebuild[*]/eix[*][:*])
OVERLAY_CACHE_METHOD="parse|ebuild*"

# STRING
# This variable is only used for delayed substitution in CACHE_METHOD.
# It is meant to be a local addition to CACHE_METHOD.
ADD_CACHE_METHOD=""

# STRING
# Overrides OVERLAY_CACHE_METHOD or PORTDIR_CACHE_METHOD for particular paths.
# This is a list of pairs DIR-PATTERN METHOD. Later entries take precedence.
CACHE_METHOD="%{ADD_CACHE_METHOD}"

# STRING
# This variable is only used for delayed substitution in OVERRIDE_CACHE_METHOD.
# It is meant to be a local addition to OVERRIDE_CACHE_METHOD.
ADD_OVERRIDE_CACHE_METHOD=""

# STRING
# This variable can override the choices of CACHE_METHOD,
# and in addition it can override the choices made by KEEP_VIRTUALS.
OVERRIDE_CACHE_METHOD="%{ADD_OVERRIDE_CACHE_METHOD}"

# STRING
# This is a list of pairs DIR-PATTERN OVERLAY_LABEL.
# When a new cachefile is created, the overlay matching DIR-PATTERN obtains
# the label OVERLAY_LABEL, independent of the content of profiles/repo_name
# or the label associated by KEEP_VIRTUALS.
# The last matching DIR_PATTERN takes precedence.
REPO_NAMES=""

# STRING
# List of overlays that should be excluded from the index.
EXCLUDE_OVERLAY=""

# STRING
# List of overlays that should be added to the index.
ADD_OVERLAY=""

# BOOLEAN
# If true and overlays are excluded or added, export modified PORTDIR_OVERLAY.
EXPORT_PORTDIR_OVERLAY="true"

# BOOLEAN
# Keep virtuals of the old cache file by adding corresponding entries
# implicitly to the values of ADD_OVERLAY and ADD_CACHE_METHOD
KEEP_VIRTUALS="false"

# BOOLEAN
# If false, /etc/portage and ACCEPT_KEYWORDS are ignored.
LOCAL_PORTAGE_CONFIG="true"

# BOOLEAN
# If true, ACCEPT_KEYWORDS is used even without LOCAL_PORTAGE_CONFIG,
# e.g. to determine the "default" stability.
ALWAYS_ACCEPT_KEYWORDS="false"

# BOOLEAN
# If +/-, eix -u will match as if LOCAL_PORTAGE_CONFIG=true/false.
UPGRADE_LOCAL_MODE=""

# BOOLEAN
# If +/-, recommendations for up- or downgrade will act as if
# LOCAL_PORTAGE_CONFIG=true/false.
RECOMMEND_LOCAL_MODE=""

# BOOLEAN
# Are packages/sets in included sets part of the parent set?
RECURSIVE_SETS="true"

# BOOLEAN
# If true, upgrade tests succeed for installed packages with new higher slots.
# Use the files SLOT_UPGRADE_FORBID or SLOT_UPGRADE_ALLOW, respectively,
# to specify exceptions.
UPGRADE_TO_HIGHEST_SLOT="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# It defines whether all information lines are printed (even if empty).
PRINT_ALWAYS="false"

# BOOLEAN
# If false, count independently of whether the matches are printed.
COUNT_ONLY_PRINTED="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# It defines whether a bug reference is printed in verbose format.
PRINT_BUGS="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# It defines whether eix-diff will output installed versions.
DIFF_PRINT_INSTALLED="true"

# STRING
# This variable is only used for delayed substitution.
# It is the command used to print the package set names.
PRINT_SETNAMES="%{?ALL_SETNAMES}all%{}setnames"

# BOOLEAN
# This variable is only used for delayed substitution.
# If false, no slot information is printed.
PRINT_SLOTS="true"

# BOOLEAN
# This variable is only used for delayed substitution in PRINT_IUSE.
# If false, no IUSE data is printed for eix. See also VERSION_IUSE_*
EIX_PRINT_IUSE="true"

# BOOLEAN
# This variable is only used for delayed substitution in PRINT_IUSE.
# If false, no IUSE data is printed for eix-diff.
DIFF_PRINT_IUSE="false"

# BOOLEAN
# This variable is only used for delayed substitution in PRINT_IUSE.
# It is unused by default.
UPDATE_PRINT_IUSE="false"

# BOOLEAN
# This variable is only used for delayed substitution in PRINT_IUSE.
# It is unused by default.
DROP_PRINT_IUSE="false"

# BOOLEAN
# This variable is only used for delayed substitution.
# If false, no IUSE data is printed.
PRINT_IUSE="%{*PRINT_IUSE}"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, eix --versionlines (nonverbose) outputs IUSE for each version.
VERSION_IUSE_NORMAL="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, eix --versionlines --verbose outputs IUSE for each version.
VERSION_IUSE_VERBOSE="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If false, KEYWORDS are never output.
PRINT_KEYWORDS="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, eix --versionlines (nonverbose) outputs KEYWORDS.
VERSION_KEYWORDS_NORMAL="false"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, eix --versionlines --verbose outputs KEYWORDS.
VERSION_KEYWORDS_VERBOSE="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If false, mask reasons are never output.
PRINT_MASKREASONS="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, eix --versionlines (nonverbose) outputs mask reasons.
MASKREASONS_NORMAL="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, eix --versionlines --verbose outputs mask reasons.
MASKREASONS_VERBOSE="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, eix --versionlines (nonverbose) outputs {,R,P}DEPEND.
VERSION_DEPS_NORMAL="false"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, eix --versionlines --verbose outputs {,R,P}DEPEND.
VERSION_DEPS_VERBOSE="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# Print effective keywords (modified by profile) instead of KEYWORDS.
USE_EFFECTIVE_KEYWORDS="false"

# BOOLEAN
# This variable is only used for delayed substitution.
# It defines whether to include "system" in package sets output.
ALL_SETNAMES="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, compact format prints no version if none is installable.
NOBEST_COMPACT="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, compact format prints no versions if all installable vanished.
NOBEST_CHANGE="true"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, eix-diff prints no version if none is installable.
DIFF_NOBEST="false"

# BOOLEAN
# This variable is only used for delayed substitution.
# If true, eix-diff prints no versions if all installable vanished.
DIFF_NOBEST_CHANGE="false"

# STRING
# The current terminal. Usually this is set by the environment variable.
TERM="xterm-256color"
# changed locally, default was:
# TERM="xterm"

# STRING
# If the beginning of TERM matches a word of this space-separated list,
# it is assumed that the terminal supports a status line.
TERM_STATUSLINE="xterm screen rxvt aterm konsole gnome Eterm eterm kterm interix"

# STRING
# If the beginning of TERM matches a word of this space-separated list, and
# if a status line is active, also a soft status line will be output.
TERM_SOFTSTATUSLINE="screen"

# STRING
# If this is nonempty, it is used as the exit statusline.
# An optional leading space in this string is ignored.
EXIT_STATUSLINE=""

# BOOLEAN
# This variable is only used for delayed substitution.
# It decides whether background colors are reset on every newline.
# This may be desired if you set BG? to a value differrent than "none".
RESET_ALL_LINES="true"

# STRING
# This variable is only used for delayed substitution.
# It is the background color for color scheme 0. Use "none" for no change.
BG0="none"

# STRING
# This variable is only used for delayed substitution.
# It is the background color for color scheme 1. Use "none" for no change.
BG1="black"

# STRING
# This variable is only used for delayed substitution.
# It is the background color for color scheme 2. Use "none" for no change.
BG2="none"

# STRING
# This variable is only used for delayed substitution.
# It is the background color for color scheme 3. Use "none" for no change.
BG3="white"

# STRING
# This variable is only used for delayed substitution.
# It is used instead of BG0 to ignore BG0 if SOLARIZED is set.
BG0S="%{!SOLARIZED}%{BG0}%{else}none%{}"

# STRING
# This is a list of regular expressions; if one of it matches TERM, then
# COLORSCHEME1 is used instead of COLORSCHEME0. The intention is that the
# specified terminals default to 256 colors.
TERM_ALT1="256 [aeEkx]term konsole gnome putty %{TERM_ALT1_ADD}"

# STRING
# This variable is only used for delayed substitution of TERM_ALT1.
TERM_ALT1_ADD=""

# STRING
# This is a list of regular expressions; if one of it matches TERM, then
# COLORSCHEME2 is used instead of COLORSCHEME0. The intention is that the
# specified terminals default to 88 colors.
TERM_ALT2="88 rxvt-unicode[^2]*\$ %{TERM_ALT2_ADD}"

# STRING
# This variable is only used for delayed substitution of TERM_ALT2.
TERM_ALT2_ADD=""

# STRING
# This is a list of regular expressions; if one of it matches TERM, then
# COLORSCHEME3 is used instead of COLORSCHEME0. The intention is that
# these terminals are exceptions made on user request.
TERM_ALT3=""

# STRING
# The value "true" or "false" means that the first or second entry of the
# COLORSCHEME* variables is used (meaning black or white background, resp.).
# The value "auto" means a heuristic based on TERM_DARK and COLORFGBG_DARK.
DARK="auto"

# STRING
# A list of pairs regexp1 darkmode1 regexp2 darkmode2 ... [default darkmode].
# If DARK=auto the first darkmode for which TERM matches regexp is assumed.
# In modes true* and false* also COLORGBGB is respected by COLORFGBG_DARK.
TERM_DARK="linux true cygwin true putty true true*"

# STRING
# This is a list of regular expressions.
# If modes true* and false* are active in TERM_DARK and COLORFGBG is nonempty,
# then the darkmode depends on whether COLORFGBG matches at least one entry.
COLORFGBG_DARK=";0?[02-689]\$ ;1[0-46]\$ ;[a-km-vxz][a-z]*\$"

# STRING
# Usually, this is set in the environment by some terminals like rxvt.
# It is only used if DARK=auto, see COLORFGBG_BLACK.
COLORFGBG=""

# BOOLEAN
# This variable is only used in delayed substitution in COLORSCHEME?.
# Set it (e.g. in your environment) if your terminal is configured to use
# the solarized color scheme of Ethan Schoonover.
#SOLARIZED="false"
SOLARIZED="true"

# STRING
# This is one or two numbers. If TERM_ALT? does not match, this chooses the
# corresponding number in color specifications (starting from 0).
# If two numbers are specified, the choice depends on DARK.
# The intention of this variable is to select the color scheme used for
# 8/16 color terminals.
COLORSCHEME0="%{!SOLARIZED}0 2%{else}5%{}"

# STRING
# This is one or two numbers. If TERM_ALT1 matches, this chooses the
# corresponding number in color specifications (starting from 0).
# If two numbers are specified, the choice depends on DARK.
# The intention of this variable is to select the color scheme used for
# 256 color terminals.
COLORSCHEME1="%{!SOLARIZED}1 3%{else}4%{}"

# STRING
# This is one or two numbers. If TERM_ALT2 matches, this chooses the
# corresponding number in color specifications (starting from 0).
# If two numbers are specified, the choice depends on DARK.
# The intention of this variable is to select the color scheme used for
# 88 color terminals.
COLORSCHEME2="%{COLORSCHEME0}"

# STRING
# This is one or two numbers. If TERM_ALT3 matches, this chooses the
# corresponding number in color specifications (starting from 0).
# If two numbers are specified, the choice depends on DARK.
# The intention of this variable is to select the color scheme used for
# user-specified terminals.
COLORSCHEME3="%{COLORSCHEME0}"

# BOOLEAN
# If false, sort installed useflags by whether they are set.
SORT_INST_USE_ALPHA="false"

# STRING
# Allowed values are true/false/repository.
# If true, always check from which overlay a package version was installed.
# If false, only packages with versions in at least two trees are checked.
# The compromise - repository - checks at least always the repository files.
# Without a check, the assumed overlay may be wrong if the version was
# actually installed from a place not in the database anymore.
CHECK_INSTALLED_OVERLAYS="repository"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_FIELD for description.
MATCH_FIELD_DESCRIPTION="[ ]"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_FIELD for set.
MATCH_FIELD_SET="[@]"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_FIELD for homepage.
MATCH_FIELD_HOMEPAGE="http.*:"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_FIELD for category/name.
MATCH_FIELD_CATEGORY_NAME="/"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_FIELD for license.
MATCH_FIELD_LICENSE="GPL|BSD|Art"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_FIELD for license.
MATCH_FIELD_DEPS="[<>=!]"

# STRING
# This is a list of strings of the form regexp[ ]match_field.
# If regexp matches the search pattern, use match_field as the default.
# A fallback match_field may be specified as the last entry in the list.
# Admissible values for match_field are: name, category, category/name,
# description, license, homepage, set, slot, installed-slot, use
# with-use, without-use.
DEFAULT_MATCH_FIELD="%{\\MATCH_FIELD_DESCRIPTION} description %{\\MATCH_FIELD_SET} set %{\\MATCH_FIELD_HOMEPAGE} homepage %{\\MATCH_FIELD_CATEGORY_NAME} category/name %{\\MATCH_FIELD_LICENSE} license %{\\MATCH_FIELD_DEPS} deps name"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_ALGORITHM for regex.
MATCH_ALGORITHM_REGEX="[][^\$|()]|[.][*+?]"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_ALGORITHM for pattern.
MATCH_ALGORITHM_PATTERN1="^[*]|[^][().][*]"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_ALGORITHM for substring.
MATCH_ALGORITHM_SUBSTRING="[^][().][+?]"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_ALGORITHM for exact.
MATCH_ALGORITHM_EXACT="^[@]"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_ALGORITHM for pattern.
MATCH_ALGORITHM_PATTERN2=":"

# STRING
# This variable is only used for delayed substitution.
# It is a regular expression used in DEFAULT_MATCH_ALGORITHM for begin.
MATCH_ALGORITHM_BEGIN="/"

# STRING
# This is a list of strings of the form regexp[ ]match_algorithm.
# If regexp matches the search pattern, use match_algorithm as the default.
# A fallback match_algorithm may be specified as the last entry in the list.
# Admissible values for match_algorithm are: regex, pattern, substring,
# begin, end, exact, fuzzy.
DEFAULT_MATCH_ALGORITHM="%{\\MATCH_ALGORITHM_REGEX} regex %{\\MATCH_ALGORITHM_PATTERN1} pattern %{\\MATCH_ALGORITHM_SUBSTRING} substring %{\\MATCH_ALGORITHM_EXACT} exact %{\\MATCH_ALGORITHM_PATTERN2} pattern %{\\MATCH_ALGORITHM_BEGIN} begin regex"

# BOOLEAN
# Defines whether empty entries in /etc/portage/package.* are shown with -t.
TEST_FOR_EMPTY="true"

# BOOLEAN
# Defines whether /etc/portage/package.accept_keywords is tested with -t.
TEST_KEYWORDS="true"


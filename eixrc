FORMATLINE_PACKAGESETS="%{!PRINT_ALWAYS}{%{PRINT_SETNAMES}}%{}\\C<%{COLUMN_TITLE}>(%{COLOR_TITLE})Package sets:(%{COLOR_RESET})%{?PRINT_ALWAYS}{%{PRINT_SETNAMES}}%{}\\C<%{COLUMN_CONTENT}>(%{COLOR_PACKAGESETS})<%{PRINT_SETNAMES}>%{!RESET_ALL_LINES}(%{COLOR_RESET})%{}%{?PRINT_ALWAYS}{}%{FORMAT_NEWLINE}%{else}%{FORMAT_NEWLINE}{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a line with the package homepage.
FORMATLINE_HOMEPAGE="%{!PRINT_ALWAYS}{homepage}%{}\\C<%{COLUMN_TITLE}>(%{COLOR_TITLE})Homepage:(%{COLOR_RESET})%{?PRINT_ALWAYS}{homepage}%{}\\C<%{COLUMN_CONTENT}>(%{COLOR_NORMAL})<homepage>%{!RESET_ALL_LINES}(%{COLOR_NORMAL_END})%{}%{?PRINT_ALWAYS}{}%{FORMAT_NEWLINE}%{else}%{FORMAT_NEWLINE}{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a line with the package bug-reference.
FORMATLINE_BUGS="%{?PRINT_BUGS}\\C<%{COLUMN_TITLE}>(%{COLOR_TITLE})Find open bugs:(%{COLOR_RESET})\\C<%{COLUMN_CONTENT}>(%{COLOR_NORMAL})http://bugs.gentoo.org/buglist.cgi?quicksearch=<category>%2F<name>%{!RESET_ALL_LINES}(%{COLOR_NORMAL_END})%{}%{FORMAT_NEWLINE}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a line with the package description.
FORMATLINE_DESCRIPTION="%{!PRINT_ALWAYS}{description}%{}\\C<%{COLUMN_TITLE}>(%{COLOR_TITLE})Description:(%{COLOR_RESET})%{?PRINT_ALWAYS}{description}%{}\\C<%{COLUMN_CONTENT}>(%{COLOR_NORMAL})<description>%{!RESET_ALL_LINES}(%{COLOR_NORMAL_END})%{}%{?PRINT_ALWAYS}{}%{FORMAT_NEWLINE}%{else}%{FORMAT_NEWLINE}{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a line with the best versions/slots.
FORMATLINE_BEST="%{!PRINT_ALWAYS}{havebest}%{}\\C<%{COLUMN_TITLE}>(%{COLOR_TITLE})Best versions/slot:(%{COLOR_RESET})%{?PRINT_ALWAYS}{havebest}%{}\\C<%{COLUMN_CONTENT}><bestslotversions:VSORT>%{?PRINT_ALWAYS}{}%{FORMAT_NEWLINE}%{else}%{FORMAT_NEWLINE}{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a line with the up-/downgrade recommendations.
FORMATLINE_RECOMMEND="%{!PRINT_ALWAYS}{recommend}%{}\\C<%{COLUMN_TITLE}>(%{COLOR_TITLE})Recommendation:(%{COLOR_RESET})%{?PRINT_ALWAYS}{recommend}%{}\\C<%{COLUMN_CONTENT}>{upgrade}(%{COLOR_UPGRADE_TEXT})Upgrade(%{COLOR_RESET}){downgrade} and {}{}{downgrade}(%{COLOR_DOWNGRADE_TEXT})Downgrade%{!RESET_ALL_LINES}(%{COLOR_RESET})%{}{}%{?PRINT_ALWAYS}{}%{FORMAT_NEWLINE}%{else}%{FORMAT_NEWLINE}{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a line with the package licenses.
FORMATLINE_LICENSES="%{!PRINT_ALWAYS}{licenses}%{}\\C<%{COLUMN_TITLE}>(%{COLOR_TITLE})License:(%{COLOR_RESET})%{?PRINT_ALWAYS}{licenses}%{}\\C<%{COLUMN_CONTENT}>(%{COLOR_NORMAL})<licenses>%{!RESET_ALL_LINES}(%{COLOR_NORMAL_END})%{}%{?PRINT_ALWAYS}{}%{FORMAT_NEWLINE}%{else}%{FORMAT_NEWLINE}{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for eix-diff after the versions.
DIFF_FORMATLINE="%{FORMAT_OVERLAYKEY}(%{COLOR_NORMAL}): <description>(%{COLOR_NORMAL_END})%{FORMAT_FINISH}"

# STRING
# This format is only used for delayed substitution in FORMAT.
# It defines the format of the normal output of eix.
FORMAT_ALL="{*modus=normal}%{FORMATLINE_NAME}%{FORMATLINE_AVAILABLEVERSIONS}%{FORMATLINE_INSTALLEDVERSIONS}%{FORMATLINE_MARKEDVERSIONS}%{FORMATLINE_HOMEPAGE}%{FORMATLINE_DESCRIPTION}%{FORMAT_FINISH}"

# STRING
# This format is only used for delayed substitution in FORMAT_COMPACT.
# It defines the format of the compact output of eix (option -c).
FORMAT_ALL_COMPACT="{*modus=compact}%{FORMATLINE_NAME_COMPACT} (%{COLOR_NORMAL})\\((%{COLOR_NORMAL_END}){havemarkedversion}<markedversions:MARKEDVERSION>; {}{installed}%{INSTALLEDVERSIONS_COMPACT}{recommend} -> %{FORMAT_BEST_CHANGE}{}{else}%{FORMAT_BEST_COMPACT}{}(%{COLOR_NORMAL})\\): <description>(%{COLOR_NORMAL_END})%{FORMAT_FINISH}"

# STRING
# This format is only used for delayed substitution in FORMAT_VERBOSE.
# It defines the format of the verbose output of eix (option -v).
FORMAT_ALL_VERBOSE="{*modus=verbose}%{FORMATLINE_NAME_VERBOSE}%{FORMATLINE_AVAILABLEVERSIONS}%{FORMATLINE_INSTALLEDVERSIONS}%{FORMATLINE_BEST}%{FORMATLINE_RECOMMEND}%{FORMATLINE_MARKEDVERSIONS}%{FORMATLINE_PACKAGESETS}%{FORMATLINE_HOMEPAGE}%{FORMATLINE_BUGS}%{FORMATLINE_DESCRIPTION}%{FORMATLINE_LICENSES}%{FORMAT_FINISH}"

# STRING
# The format of the normal output of eix.
# Do not modify it in a config file; modify FORMAT_ALL instead.
FORMAT="%{FORMAT_ALL}"

# STRING
# The format of the compact output of eix (option -c).
# Do not modify it in a config file; modify FORMAT_ALL_COMPACT instead.
FORMAT_COMPACT="%{FORMAT_ALL_COMPACT}"

# STRING
# The format of the verbose output of eix (option -v).
# Do not modify it in a config file; modify FORMAT_ALL_VERBOSE instead.
FORMAT_VERBOSE="%{FORMAT_ALL_VERBOSE}"

# STRING
# The format used for output by the eix-test-obsolete script.
# The value %{FORMAT} is not allowed here; use %{FORMAT_ALL} instead.
FORMAT_TEST_OBSOLETE="%{FORMAT_ALL_COMPACT}"

# STRING
# This format is only used for delayed substitution in DIFF_FORMAT_NEW.
# It defines the format used for new packages (eix-diff).
DIFF_FORMAT_ALL_NEW="%{DIFF_FORMATLINE_NAME_NEW}%{DIFF_FORMATLINE_BEST}%{DIFF_FORMATLINE}"

# STRING
# This format is only used for delayed substitution in DIFF_FORMAT_DELETE.
# It defines the format used for packages that were deleted (eix-diff).
DIFF_FORMAT_ALL_DELETE="%{DIFF_FORMATLINE_NAME_DELETE}%{DIFF_FORMATLINE_BEST}%{DIFF_FORMATLINE}"

# STRING
# This format is only used for delayed substitution in DIFF_FORMAT_CHANGED.
# It defines the format used for packages that were changed (eix-diff).
DIFF_FORMAT_ALL_CHANGED="%{DIFF_FORMATLINE_NAME_CHANGED}%{DIFF_FORMATLINE_CHANGED_VERSIONS}%{DIFF_FORMATLINE}"

# STRING
# The format used for new packages (eix-diff).
# Do not modify it in a config file; modify DIFF_FORMAT_ALL_NEW instead.
DIFF_FORMAT_NEW="%{DIFF_FORMAT_ALL_NEW}"

# STRING
# The format used for packages that were deleted (eix-diff).
# Do not modify it in a config file; modify DIFF_FORMAT_ALL_DELETE instead.
DIFF_FORMAT_DELETE="%{DIFF_FORMAT_ALL_DELETE}"

# STRING
# The format used for packages that were changed (eix-diff).
# Do not modify it in a config file; modify DIFF_FORMAT_ALL_CHANGED instead.
DIFF_FORMAT_CHANGED="%{DIFF_FORMAT_ALL_CHANGED}"

# STRING
# strftime() format for printing the installation date in long form
FORMAT_INSTALLATION_DATE="%X %x"

# STRING
# strftime() format for printing the installation date in short form
FORMAT_SHORT_INSTALLATION_DATE="%x"

# STRING
# Can be full/effective/both/full*/effective*/none.
# Depending on the value, with --xml the full/effective (or both types)
# KEYWORDS string is output for each version.
# With full*/effective* also both types are output if they differ.
XML_KEYWORDS="none"

# STRING
# If false, the overlay is not output with --xml.
# For overlays without label (repository name) the overlay is output anyway.
XML_OVERLAY="false"

# STRING
# strftime() format for printing the installation date with --xml.
XML_DATE="%s"

# STRING
# This string is printed as line separator in <maskreasons>.
FORMAT_MASKREASONS_LINESKIP="%{?WIDETERM} %{else}%{FORMAT_NEWLINE}(%{COLOR_MASKREASONS})%{}"

# STRING
# This string is printed as separator for different <maskreasons>.
FORMAT_MASKREASONS_SEP="%{?WIDETERM} - %{else}%{FORMAT_NEWLINE}%{FORMAT_NEWLINE}(%{COLOR_MASKREASONS})%{}"

# STRING
# This string is printed as line separator in <maskreasons*>.
FORMAT_MASKREASONSS_LINESKIP="%{?WIDETERM}%{FORMAT_VER_LINESKIP}\\C<%{COLUMN_AVAILABLE_CONTENT}>%{else}%{FORMAT_NEWLINE}%{}(%{COLOR_MASKREASONS})"

# STRING
# This string is printed as separator for different <maskreasons*>.
FORMAT_MASKREASONSS_SEP="%{FORMAT_NEWLINE}%{FORMAT_MASKREASONSS_LINESKIP}"

# STRING
# This string is printed before each set USE flag of an installed version.
FORMAT_BEFORE_SET_USE="(%{COLOR_SET_USE})"

# STRING
# This string is printed after each set USE flag of an installed version.
FORMAT_AFTER_SET_USE="(%{COLOR_RESET})"

# STRING
# This string is printed before each unset USE flag of an installed version.
FORMAT_BEFORE_UNSET_USE="(%{COLOR_UNSET_USE})-"

# STRING
# This string is printed after each unset USE flag of an installed version.
FORMAT_AFTER_UNSET_USE="(%{COLOR_RESET})"

# STRING
# This string is printed before the variable name of an USE_EXPAND use value.
FORMAT_BEFORE_USE_EXPAND_START="(%{COLOR_USE_EXPAND_START})"

# STRING
# This string is printed after the variable name of an USE_EXPAND use value.
FORMAT_BEFORE_USE_EXPAND_END="=\""

# STRING
# This string is printed at the end of a USE_EXPAND use value.
FORMAT_AFTER_USE_EXPAND="(%{COLOR_USE_EXPAND_START})\"(%{COLOR_USE_EXPAND_END})"

# STRING
# This string is printed before the variable name of an USE_EXPAND iuse value.
FORMAT_BEFORE_IUSE_EXPAND_START="(%{COLOR_USE_EXPAND_START})"

# STRING
# This string is printed after the variable name of an USE_EXPAND iuse value.
FORMAT_BEFORE_IUSE_EXPAND_END="=\"(%{COLOR_VERSION_IUSE})"

# STRING
# This string is printed at the end of a USE_EXPAND iuse value.
FORMAT_AFTER_IUSE_EXPAND="(%{COLOR_USE_EXPAND_START})\"(%{COLOR_USE_EXPAND_END})"

# STRING
# This string is printed before the variable name of an USE_EXPAND collected
# iuse value.
FORMAT_BEFORE_COLL_EXPAND_START="(%{COLOR_USE_EXPAND_START})"

# STRING
# This string is printed after the variable name of an USE_EXPAND collected
# iuse value.
FORMAT_BEFORE_COLL_EXPAND_END="=\"(%{COLOR_COLL_IUSE})"

# STRING
# This string is printed at the end of a USE_EXPAND collected iuse value.
FORMAT_AFTER_COLL_EXPAND="(%{COLOR_USE_EXPAND_START})\"(%{COLOR_USE_EXPAND_END})"

# STRING
# This variable is only used for delayed substitution.
# This string is printed before IUSE data for a version is output.
# (Normally, this is only used when --versionlines is active)
FORMAT_BEFORE_IUSE="{!*colliuse}\\t(%{COLOR_NORMAL})[(%{COLOR_NORMAL_END})"

# STRING
# This variable is only used for delayed substitution.
# This string is printed after IUSE data for a version is output.
# (Normally, this is only used when --versionlines is active)
FORMAT_AFTER_IUSE="(%{COLOR_NORMAL})](%{COLOR_NORMAL_END})"

# STRING
# This variable is only used for delayed substitution.
# This string is printed before IUSE data for all versions is output.
# (This is meant for printing after all versions in a line)
FORMAT_BEFORE_COLL="{*colliuse} (%{COLOR_USE_COLL})\\{"

# STRING
# This variable is only used for delayed substitution.
# This string is printed after IUSE data for all versions is output.
# (This is meant for printing after all versions in a line)
FORMAT_AFTER_COLL="(%{COLOR_USE_COLL})\\}(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# This string is printed before IUSE data for all versions is output.
# (This is meant for printing in a separate line)
FORMAT_BEFORE_COLL_SEP="%{FORMAT_NEWLINE}\\C<%{COLUMN_USE}>(%{COLOR_USE_COLL})\\{"

# STRING
# This variable is only used for delayed substitution.
# This string is printed after IUSE data for all versions is output.
# (This is meant for printing in a separate line)
FORMAT_AFTER_COLL_SEP="%{FORMAT_AFTER_COLL}"

# STRING
# This variable is only used for delayed substitution.
# It defines the color for masked versions.
COLOR_MASKED="red;%{BG0S}|196;%{BG1}|red;%{BG2}|196;%{BG3}"

# STRING
# This variable is only used for delayed substitution.
# It defines the color for unstable versions.
COLOR_UNSTABLE="yellow;%{BG0S}|190;%{BG1}|blue;%{BG2}|21;%{BG3}"

# STRING
# This variable is only used for delayed substitution.
# It defines the color for stable versions.
COLOR_STABLE="green;%{BG0S}|46;%{BG1}|;%{BG2}|58;%{BG3}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format to change the color for masked versions.
FORMAT_COLOR_MASKED="(%{COLOR_MASKED})"

# STRING
# This variable is only used for delayed substitution.
# It defines the format to change the color for unstable versions.
FORMAT_COLOR_UNSTABLE="(%{COLOR_UNSTABLE})"

# STRING
# This variable is only used for delayed substitution.
# It defines the format to change the color for stable versions.
FORMAT_COLOR_STABLE="(%{COLOR_STABLE})"

# STRING
# This variable is ignored. You can use it to collect delayed references to
# locally added (unused) variables so that they are printed with --dump.
DUMMY=""


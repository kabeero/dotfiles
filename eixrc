MARK_VERSIONS="underline"

# BOOLEAN
# This variable is only used for delayed substitution.
# It is the default for NO{COLORS,STATUSLINE,PERCENTAGE}.
NOCOLOR="false"

# STRING
# This variable is only used for delayed substitution.
# It defines the character printed for upgrade recommendations.
CHAR_UPGRADE="U"

# STRING
# This variable is only used for delayed substitution.
# It defines the character printed for downgrade recommendations.
CHAR_DOWNGRADE="D"

# STRING
# This variable is only used for delayed substitution.
# It defines the character printed for installed packages.
CHAR_INSTALLED="I"

# STRING
# This variable is only used for delayed substitution.
# It defines the character printed for uninstalled packages.
CHAR_UNINSTALLED="%{DIFF_CHAR_NEW}"

# STRING
# This variable is only used for delayed substitution.
# It defines the character printed for uninstalled packages which have gained
# a stable version.
DIFF_CHAR_UNINST_STABILIZE="%{DIFF_CHAR_INST_STABILIZE}"

# STRING
# This variable is only used for delayed substitution.
# It defines the character printed for installed packages which have gained
# a stable version.
DIFF_CHAR_INST_STABILIZE="*"

# STRING
# This variable is only used for delayed substitution.
# It defines the character printed for new packages (eix-diff).
DIFF_CHAR_NEW="N"

# STRING
# This variable is only used for delayed substitution.
# It defines the character used for "better version" tags (eix-diff).
DIFF_CHAR_BETTER=">"

# STRING
# This variable is only used for delayed substitution.
# It defines the character used for "worse version" tags (eix-diff).
DIFF_CHAR_WORSE="\\<"

# STRING
# This variable is only used for delayed substitution.
# It defines the tag printed for upgrade recommendations.
TAG_UPGRADE="(%{COLOR_UPGRADE})%{CHAR_UPGRADE}(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines the tag printed for downgrade recommendations.
TAG_DOWNGRADE="(%{COLOR_DOWNGRADE})%{CHAR_DOWNGRADE}(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines the tag printed for installed packages.
TAG_INSTALLED="(%{COLOR_INST_TAG})%{CHAR_INSTALLED}(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines the tag printed for uninstalled packages.
TAG_UNINSTALLED="(%{COLOR_UNINST_TAG})%{CHAR_UNINSTALLED}(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines the tag for packages which have gained a stable version.
TAG_STABILIZE="{installed}(%{DIFF_COLOR_INST_STABILIZE})%{DIFF_CHAR_INST_STABILIZE}{else}(%{DIFF_COLOR_UNINST_STABILIZE})%{DIFF_CHAR_UNINST_STABILIZE}{}(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines the tag printed for new packages (eix-diff).
TAG_NEW="(%{DIFF_COLOR_NEW_TAG})%{DIFF_CHAR_NEW}(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines the tag used for "better version" (eix-diff).
TAG_BETTER="(%{DIFF_COLOR_BETTER})%{DIFF_CHAR_BETTER}(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines the tag used for "worse version" (eix-diff).
TAG_WORSE="(%{DIFF_COLOR_WORSE})%{DIFF_CHAR_WORSE}(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines the string used for "plain" tagging installed packages.
STRING_PLAIN_INSTALLED="%{STRING_PLAIN_UNINSTALLED}"

# STRING
# This variable is only used for delayed substitution.
# It defines the string used for "plain" tagging uninstalled packages.
STRING_PLAIN_UNINSTALLED="*"

# STRING
# This variable is only used for delayed substitution.
# It defines the string used for "new package" separators (eix-diff).
DIFF_STRING_NEW=">>"

# STRING
# This variable is only used for delayed substitution.
# It defines the string used for "deleted package" separators (eix-diff).
DIFF_STRING_DELETE="\\<\\<"

# STRING
# This variable is only used for delayed substitution.
# It defines the string used for "changed package" separators (eix-diff).
DIFF_STRING_CHANGED="=="

# STRING
# This variable is only used for delayed substitution.
# It defines what to print if no version number is printed.
FORMAT_NOBEST="%{FORMAT_COLOR_MASKED}--(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines what to print after "->" if there is no installable.
FORMAT_NOBEST_CHANGE="%{FORMAT_COLOR_MASKED}??(%{COLOR_RESET})"

# STRING
# Tag for versions with *.tbz2. This is only used for delayed substitution.
TAG_BINARY="\\{tbz2}"

# STRING
# Tag for RESTRICT=fetch. This is only used for delayed substitution.
TAG_RESTRICT_FETCH="f"

# STRING
# Tag for RESTRICT=mirror. This is only used for delayed substitution.
TAG_RESTRICT_MIRROR="m"

# STRING
# Tag for RESTRICT=primaryuri. This is only used for delayed substitution.
TAG_RESTRICT_PRIMARYURI="p"

# STRING
# Tag for RESTRICT=binchecks. This is only used for delayed substitution.
TAG_RESTRICT_BINCHECKS="b"

# STRING
# Tag for RESTRICT=strip. This is only used for delayed substitution.
TAG_RESTRICT_STRIP="s"

# STRING
# Tag for RESTRICT=test. This is only used for delayed substitution.
TAG_RESTRICT_TEST="t"

# STRING
# Tag for RESTRICT=userpriv. This is only used for delayed substitution.
TAG_RESTRICT_USERPRIV="u"

# STRING
# Tag for RESTRICT=installsources. This is only used for delayed substitution.
TAG_RESTRICT_INSTALLSOURCES="i"

# STRING
# Tag for RESTRICT=bindist. This is only used for delayed substitution.
TAG_RESTRICT_BINDIST="d"

# STRING
# Tag for RESTRICT=parallel. This is only used for delayed substitution.
TAG_RESTRICT_PARALLEL="P"

# STRING
# Tag for PROPERTIES=interactive. This is only used for delayed substitution.
TAG_PROPERTIES_INTERACTIVE="i"

# STRING
# Tag for PROPERTIES=live. This is only used for delayed substitution.
TAG_PROPERTIES_LIVE="l"

# STRING
# Tag for PROPERTIES=virtual. This is only used for delayed substitution.
TAG_PROPERTIES_VIRTUAL="v"

# STRING
# Tag for PROPERTIES=set. This is only used for delayed substitution.
TAG_PROPERTIES_SET="s"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "profile masked" versions.
TAG_FOR_PROFILE="[P]"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "package.masked" versions.
TAG_FOR_MASKED="[M]"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "originally profile masked but unmasked" versions.
TAG_FOR_EX_PROFILE="{P}"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "originally package.masked but unmasked" versions.
TAG_FOR_EX_MASKED="{M}"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "only locally masked" versions.
TAG_FOR_LOCALLY_MASKED="[m]"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "stable" versions.
TAG_FOR_STABLE=""

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "unstable" versions.
TAG_FOR_UNSTABLE="~"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "-*" versions.
TAG_FOR_MINUS_ASTERISK="-*"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "-~*" versions.
TAG_FOR_MINUS_UNSTABLE="-~*"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "-ARCH" versions.
TAG_FOR_MINUS_KEYWORD="-"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "ALIENARCH" versions.
TAG_FOR_ALIEN_STABLE="*"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "~ALIENARCH" versions.
TAG_FOR_ALIEN_UNSTABLE="~*"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "no keyword" versions.
TAG_FOR_MISSING_KEYWORD="**"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "originally unstable but now stable" versions.
TAG_FOR_EX_UNSTABLE="(~)"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "originally -* but now stable" versions.
TAG_FOR_EX_MINUS_ASTERISK="(-*)"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "originally -~* but now stable" versions.
TAG_FOR_EX_MINUS_UNSTABLE="(-~*)"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "originally -ARCH but now stable" versions.
TAG_FOR_EX_MINUS_KEYWORD="(-)"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "originally ALIENARCH but now stable" versions.
TAG_FOR_EX_ALIEN_STABLE="(*)"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "originally ~ALIENARCH but now stable" versions.
TAG_FOR_EX_ALIEN_UNSTABLE="(~*)"

# STRING
# This variable is only used for delayed substitution.
# It is the tag for "originally no keyword but now stable" versions.
TAG_FOR_EX_MISSING_KEYWORD="(**)"

# STRING
# This variable is used by delayed substitution in version formatters.
# It prints a newline at the end of a version.
VERSION_NEWLINE="\\n"

# STRING
# This variable is used as a version formatter.
# It is an example for usage with <bestslotupgradeversions:NAMEVERSION>
# or <installedversions:NAMEVERION> or <availableversions:NAMEVERSION>.
NAMEVERSION="<category>/<name>-<version>%{VERSION_NEWLINE}"

# STRING
# This variable is used as a version formatter.
# It is an example for usage with <bestslotupgradeversions:EQNAMEVERSION>
# or <installedversions:EQNAMEVERION> or <availableversions:EQNAMEVERSION>.
EQNAMEVERSION="=<category>/<name>-<version>%{VERSION_NEWLINE}"

# STRING
# This variable is used as a version formatter.
# It is an example for usage as <availableversion:ANAMESLOT:ANAMESLOT>.
ANAMESLOT="{slotlast}<category>/<name>{slots}:<slot>{}%{VERSION_NEWLINE}"

# STRING
# This variable is used as a version formatter.
# It is an example for usage with <availableversion:ANAMEASLOT:ANAMEASLOT>.
ANAMEASLOT="{slotlast}<category>/<name>:<slot>{!last}\\n{}{}%{VERSION_NEWLINE}"

# STRING
# This variable is used as a version formatter.
# It is an example for usage with <installedversions:NAMESLOT>.
NAMESLOT="<category>/<name>{slots}:<slot>{}%{VERSION_NEWLINE}"

# STRING
# This variable is used as a version formatter.
# It is an example for usage with <installedversions:NAMEASLOT>.
NAMEASLOT="<category>/<name>:<slot>%{VERSION_NEWLINE}"

# STRING
# strftime() format for printing the installation date in DATESORT
DATESORT_DATE="%s	%x %X"

# STRING
# This variable is used as a version formatter.
# It is an example for usage as <installedversions:DATESORT>. Typical usage:
# eix -'*I' --format '<installedversions:DATESORT>' | sort | cut -f2-3
DATESORT="<date:DATESORT_DATE>\\t%{NAMESLOT}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format of the DEPEND output.
FORMAT_DEPEND="(%{COLOR_DEPEND})<depend*>(%{COLOR_DEPEND_END})"

# STRING
# This variable is only used for delayed substitution.
# It defines the format of the RDEPEND output.
FORMAT_RDEPEND="(%{COLOR_DEPEND})<rdepend*>(%{COLOR_DEPEND_END})"

# STRING
# This variable is only used for delayed substitution.
# It defines the format of the PDEPEND output.
FORMAT_PDEPEND="(%{COLOR_DEPEND})<pdepend*>(%{COLOR_DEPEND_END})"

# STRING
# This variable is only used for delayed substitution.
# It defines the format of the HDEPEND output.
FORMAT_HDEPEND="(%{COLOR_DEPEND})<hdepend*>(%{COLOR_DEPEND_END})"

# STRING
# This variable is only used for delayed substitution.
# It defines the format of the DEPEND output with --verbose.
FORMAT_DEPEND_VERBOSE="%{FORMAT_DEPEND}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format of the RDEPEND output with --verbose.
FORMAT_RDEPEND_VERBOSE="%{FORMAT_RDEPEND}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format of the PDEPEND output with --verbose.
FORMAT_PDEPEND_VERBOSE="%{FORMAT_PDEPEND}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format of the HDEPEND output with --verbose.
FORMAT_HDEPEND_VERBOSE="%{FORMAT_HDEPEND}"

# STRING
# This variable is only used for delayed substitution.
# It is used for colored <use> in available versions; color is not reset.
FORMAT_VERSION_IUSE="(%{COLOR_VERSION_IUSE})%{?EIX_USE_EXPAND}<use*>%{else}<use>%{}"

# STRING
# This variable is only used for delayed substitution.
# It is used for colored <colliuse>; color is not reset.
FORMAT_COLLIUSE="(%{COLOR_COLL_IUSE})%{?EIX_USE_EXPAND}<colliuse*>%{else}<colliuse>%{}"

# STRING
# This variable is only used for delayed substitution.
# It is used for printing colored <keywords>; color is not reset.
FORMAT_KEYWORDS="(%{COLOR_KEYWORDS})<versionkeywords>(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It is used for printing colored <keywords*>; color is not reset.
FORMAT_KEYWORDSS="(%{COLOR_KEYWORDSS})<versionkeywords*>(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines what to output for KEYWORDS* if they equal KEYWORDS.
FORMAT_KEYWORDS_EQUAL="{versionkeywords}(%{COLOR_KEYWORDSS})\$\\{KEYWORDS\\}(%{COLOR_RESET}){}"

# STRING
# This variable is only used for delayed substitution.
# It sets the runtime variable $mask to the masking tag, and unless the
# runtime variable $color is set, it outputs the color and sets $color.
FORMAT_MASK_TAG="{!*mask}{washardmasked}%{?COLOR_ORIGINAL}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}%{}{isprofilemasked}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*mask=\"%{TAG_FOR_PROFILE}\"}{else}{ismasked}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*mask=\"%{TAG_FOR_MASKED}\"}{else}{wasprofilemasked}{*mask=\"%{TAG_FOR_EX_PROFILE}\"}{else}{*mask=\"%{TAG_FOR_EX_MASKED}\"}{}{}{}{else}{ishardmasked}%{?COLOR_LOCAL_MASK}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}%{}{*mask=\"%{TAG_FOR_LOCALLY_MASKED}\"}{}{}"

# STRING
# This variable is only used for delayed substitution.
# It sets the runtime variable $stable to the stability tag, and unless the
# runtime variable $color is set, it outputs the color and sets $color.
FORMAT_STABILITY_TAG="{isstable}%{!COLOR_ORIGINAL}{!\$color}{*color}%{FORMAT_COLOR_STABLE}{}%{}{wasstable}{!\$color}{*color}%{FORMAT_COLOR_STABLE}{}{*stable=\"%{TAG_FOR_STABLE}\"}{else}{wasunstable}{!\$color}{*color}%{FORMAT_COLOR_UNSTABLE}{}{*stable=\"%{TAG_FOR_EX_UNSTABLE}\"}{else}{wasmissingkeyword}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_EX_MISSING_KEYWORD}\"}{else}{wasalienstable}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_EX_ALIEN_STABLE}\"}{else}{wasalienunstable}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_EX_ALIEN_UNSTABLE}\"}{else}{wasminuskeyword}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_EX_MINUS_KEYWORD}\"}{else}{wasminusunstable}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_EX_MINUS_UNSTABLE}\"}{else}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_EX_MINUS_ASTERISK}\"}{}{}{}{}{}{}{}{else}{isunstable}{!\$color}{*color}%{FORMAT_COLOR_UNSTABLE}{}{*stable=\"%{TAG_FOR_UNSTABLE}\"}{else}{ismissingkeyword}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_MISSING_KEYWORD}\"}{else}{isalienstable}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_ALIEN_STABLE}\"}{else}{isalienunstable}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_ALIEN_UNSTABLE}\"}{else}{isminuskeyword}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_MINUS_KEYWORD}\"}{else}{isminusunstable}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_MINUS_UNSTABLE}\"}{else}{!\$color}{*color}%{FORMAT_COLOR_MASKED}{}{*stable=\"%{TAG_FOR_MINUS_ASTERISK}\"}{}{}{}{}{}{}{}"

# STRING
# This variable is only used for delayed substitution.
# It outputs the stability tag, changing the color appropriately.
# It sets the runtime variable $color depending on whether color was changed.
FORMAT_STABILITY="{!*color}%{FORMAT_MASK_TAG}%{FORMAT_STABILITY_TAG}<\$mask><\$stable>"

# STRING
# This variable is only used for delayed substitution.
# It sets the initial string and $color for PROPERTIES output.
FORMAT_PROPERTIESSEPARATOR="<\$sep>{!*sep}{*color}(%{COLOR_PROPERTIES})*"

# STRING
# This variable is only used for delayed substitution.
# It sets the initial string and $color for RESTRICT output.
FORMAT_RESTRICTSEPARATOR="<\$sep>{!*sep}{*color}(%{COLOR_RESTRICT})^"

# STRING
# This variable is only used for delayed substitution.
# It outputs the PROPERTIES tag, changing the color appropriately.
# It sets the runtime variable $color if color was changed.
FORMAT_PROPERTIES="{properties}%{FORMAT_PROPERTIESSEPARATOR}{propertiesinteractive}%{TAG_PROPERTIES_INTERACTIVE}{}{propertieslive}%{TAG_PROPERTIES_LIVE}{}{propertiesset}%{TAG_PROPERTIES_SET}{}{}"

# STRING
# This variable is only used for delayed substitution.
# It outputs the RESTRICT tag, changing the color appropriately.
# It sets the runtime variable $color if color was changed.
FORMAT_RESTRICT="{restrict}%{FORMAT_RESTRICTSEPARATOR}{restrictfetch}%{TAG_RESTRICT_FETCH}{}{restrictmirror}%{TAG_RESTRICT_MIRROR}{}{restrictprimaryuri}%{TAG_RESTRICT_PRIMARYURI}{}{restrictbinchecks}%{TAG_RESTRICT_BINCHECKS}{}{restrictstrip}%{TAG_RESTRICT_STRIP}{}{restricttest}%{TAG_RESTRICT_TEST}{}{restrictuserpriv}%{TAG_RESTRICT_USERPRIV}{}{restrictinstallsources}%{TAG_RESTRICT_INSTALLSOURCES}{}{restrictbindist}%{TAG_RESTRICT_BINDIST}{}{restrictparallel}%{TAG_RESTRICT_PARALLEL}{}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format of the PROPERTIES and RESTRICT of a version
# and resets the color.
FORMAT_PROPRESTRICT="%{!NO_RESTRICTIONS}{!*color}%{FORMAT_PROPERTIES}%{FORMAT_RESTRICT}{\$color}(%{COLOR_RESET}){}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format of the PROPERTIES and RESTRICT of a version
# and resets the color.
FORMAT_BINARY="%{!NO_BINARY}{isbinary}<\$sep>{!*sep}(%{COLOR_BINARY})%{TAG_BINARY}(%{COLOR_RESET}){}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the slot format printed for slotsorted versions.
FORMAT_SLOT="(%{COLOR_SLOTS})\\(<slot>\\)(%{COLOR_RESET})"

# STRING
# This variable is used for <markedversions:MARKEDVERSION>.
MARKEDVERSION="(%{COLOR_MARKED_VERSION})<version>(%{COLOR_RESET}){!last} {}"

# STRING
# This variable is only used for delayed substitution.
# It outputs an available version with various marks and sets the runtime
# variable $color if a mark was printed.
# It should be follows by FORMAT_VERSION_END or FORMAT_VERSIONS_END.
FORMAT_PVERSION="{color}{installedversion}{*color}(none;%{MARK_INSTALLED}){else}{isbestupgradeslot}{*color}(%{MARK_UPGRADE}){}{}{markedversion}{*color}(%{MARK_VERSIONS}){}{}<version>"

# STRING
# This variable is only used for delayed substitution.
# It resets all colors/markers if the runtime variable $color was set.
FORMAT_VERSION_END="{\$color}(%{COLOR_RESET}){}"

# STRING
# This variable is only used for delayed substitution.
# It outputs an optional subslot, caring about the runtime variable $color,
# and then invokes FORMAT_VERSION_END.
FORMAT_VERSIONO_END="%{?PRINT_SLOTS}{issubslot}%{?COLORED_SLOTS}{\$color}(%{COLOR_RESET}){}{*color}%{?COLON_SLOTS}:(%{COLOR_SLOTS})<fullslot>%{else}<\$sep>{!*sep}(%{COLOR_SLOTS})\\(<fullslot>\\)%{}%{else}%{?COLON_SLOTS}:<fullslot>%{else}<\$sep>{!*sep}\\(<fullslot>\\)%{}%{}{}%{}%{FORMAT_VERSION_END}"

# STRING
# This variable is only used for delayed substitution.
# It outputs an optional slot, caring about the runtime variable $color,
# and then invokes FORMAT_VERSION_END.
FORMAT_VERSIONS_END="%{?PRINT_SLOTS}{isfullslot}%{?COLORED_SLOTS}{\$color}(%{COLOR_RESET}){}{*color}%{?COLON_SLOTS}:(%{COLOR_SLOTS})<fullslot>%{else}<\$sep>{!*sep}(%{COLOR_SLOTS})\\(<fullslot>\\)%{}%{else}%{?COLON_SLOTS}:<fullslot>%{else}<\$sep>{!*sep}\\(<fullslot>\\)%{}%{}{}%{}%{FORMAT_VERSION_END}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a plain version without slot.
PVERSION="%{FORMAT_PVERSION}%{FORMAT_VERSION_END}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for printing a plain version with its slot.
PVERSIONO="%{FORMAT_PVERSION}%{FORMAT_VERSIONO_END}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for printing a plain version with its slot.
PVERSIONS="%{FORMAT_PVERSION}%{FORMAT_VERSIONS_END}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for an available version without slot.
AVERSION="%{FORMAT_STABILITY}%{FORMAT_PVERSION}%{FORMAT_VERSION_END}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for printing an available version
# with its optional subslot.
AVERSIONO="%{FORMAT_STABILITY}%{FORMAT_PVERSION}%{FORMAT_VERSIONO_END}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for printing an available version with its slot.
AVERSIONS="%{FORMAT_STABILITY}%{FORMAT_PVERSION}%{FORMAT_VERSIONS_END}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for printing an installed version with its slot.
IVERSIONS="(%{COLOR_INST_VERSION}){*color}<version>%{FORMAT_VERSIONS_END}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for printing the overlay in versions.
OVERLAYVER="{overlayver}<\$sep>{!*sep}<overlayver>{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a plain version with most data and slot.
PVERSIONS_VERBOSE="%{PVERSIONS}%{FORMAT_PROPRESTRICT}%{FORMAT_BINARY}%{OVERLAYVER}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for an available version with most data and slot.
AVERSIONS_VERBOSE="%{AVERSIONS}%{FORMAT_PROPRESTRICT}%{FORMAT_BINARY}%{OVERLAYVER}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for an installed version with most data and slot.
IVERSIONS_VERBOSE="%{IVERSIONS}%{FORMAT_PROPRESTRICT}%{FORMAT_BINARY}%{OVERLAYVER}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a plain version with most data, optional subslot.
PVERSIONO_VERBOSE="%{PVERSIONO}%{FORMAT_PROPRESTRICT}%{FORMAT_BINARY}%{OVERLAYVER}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for an available version with most data,
# optional subslot.
AVERSIONO_VERBOSE="%{AVERSIONO}%{FORMAT_PROPRESTRICT}%{FORMAT_BINARY}%{OVERLAYVER}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a plain version with most data, no slot.
PVERSION_VERBOSE="%{PVERSION}%{FORMAT_PROPRESTRICT}%{FORMAT_BINARY}%{OVERLAYVER}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for an available version with most data, no slot.
AVERSION_VERBOSE="%{AVERSION}%{FORMAT_PROPRESTRICT}%{FORMAT_BINARY}%{OVERLAYVER}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a plain version with important data and slot.
PVERSIONS_COMPACT="%{PVERSIONS}%{FORMAT_BINARY}%{OVERLAYVER}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for an available version with important data and slot.
AVERSIONS_COMPACT="%{AVERSIONS}%{FORMAT_BINARY}%{OVERLAYVER}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for an installed version with important data and slot.
IVERSIONS_COMPACT="%{IVERSIONS}%{FORMAT_BINARY}%{OVERLAYVER}"

# STRING
# This variable is only used for delayed substitution.
# This string is printed before KEYWORDS string for a version is output
# (with --versionlines and nonverbose)
FORMAT_BEFORE_KEYWORDS="(%{COLOR_NORMAL}) \\C<%{COLUMN_KEYWORDS}>\""

# STRING
# This variable is only used for delayed substitution.
# This string is printed after KEYWORDS string for a version is output.
# (with --versionlines and nonverbose)
FORMAT_AFTER_KEYWORDS="(%{COLOR_NORMAL})\"(%{COLOR_NORMAL_END})"

# STRING
# This variable only used for delayed substitution.
# It prints a newline, optionally handling background colors for broken terminals.
FORMAT_NEWLINE="%{?RESET_ALL_LINES}()\\n(%{COLOR_RESET_BG})%{else}\\n%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column for the titles.
COLUMN_TITLE="5"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column after the version.
COLUMN_KEYWORDS="22"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column for the content.
COLUMN_CONTENT="26"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column for the title of installed versions.
COLUMN_INST_TITLE="%{COLUMN_CONTENT}"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column for the content of installed versions.
COLUMN_INST_CONTENT="37"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column for the title of available versions.
COLUMN_AVAILABLE_TITLE="15"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column for the content of available versions.
COLUMN_AVAILABLE_CONTENT="26"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column for the slot without versionlines.
COLUMN_SLOT="%{COLUMN_TITLE}"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column after the slot without versionlines.
COLUMN_SLOT_AFTER="12"

# STRING
# This variable is only used for delayed substitution.
# It defines the first stability column with versionlines.
COLUMN_STABILITY="7"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column after stability with versionlines.
COLUMN_STABILITY_AFTER="12"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column of the slot with versionsort.
COLUMN_SLOT_APPEND="22"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column after the version without versionsort.
COLUMN_VER_APPEND="22"

# STRING
# This variable is only used for delayed substitution.
# It defines the first column of the collected USE.
COLUMN_USE="7"

# STRING
# This variable is only used for delayed substitution.
# It defines the column for the main tag in eix-diff.
DIFF_COLUMNA="6"

# STRING
# This variable is only used for delayed substitution.
# It defines the column after the tags in eix-diff.
DIFF_COLUMNB="9"

# STRING
# This variable is only used for delayed substitution.
# It defines the lineskip used for the versionline appendix.
FORMAT_VER_LINESKIP="%{FORMAT_NEWLINE}\\C<%{COLUMN_AVAILABLE_TITLE}>"

# STRING
# This variable is only used for delayed substitution.
# It defines the normal format for KEYWORDS for an available version.
FORMAT_VERSION_KEYWORDS_NORMAL="%{?PRINT_KEYWORDS}%{?VERSION_KEYWORDS_NORMAL}%{FORMAT_BEFORE_KEYWORDS}%{?USE_EFFECTIVE_KEYWORDS}%{FORMAT_KEYWORDSS}%{else}%{FORMAT_KEYWORDS}%{}%{FORMAT_AFTER_KEYWORDS}%{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the normal format for IUSE for an available version.
FORMAT_IUSE_NORMAL="%{?PRINT_IUSE}%{?VERSION_IUSE_NORMAL}{haveuse}%{FORMAT_BEFORE_IUSE}%{FORMAT_VERSION_IUSE}%{FORMAT_AFTER_IUSE}{}%{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the verbose format for DEPs for an available version.
FORMAT_DEPS_NORMAL="%{?DEP}%{?VERSION_DEPS_NORMAL}{havedepend}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})DEPEND:(%{COLOR_RESET})\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_DEPEND}{}{haverdepend}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})RDEPEND:(%{COLOR_RESET})\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_RDEPEND}{}{havepdepend}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})PDEPEND:(%{COLOR_RESET})\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_PDEPEND}{}{havehdepend}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})HDEPEND:(%{COLOR_RESET})\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_HDEPEND}{}%{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the normal format for mask reasons for an available version.
FORMAT_MASKREASONS_NORMAL="%{?PRINT_MASKREASONS}%{?MASKREASONS_NORMAL}{havemaskreasons}%{?WIDETERM} %{else}%{FORMAT_NEWLINE}%{}(%{COLOR_MASKREASONS})<maskreasons>(%{COLOR_RESET}){}%{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the verbose format for KEYWORDS for an available version.
FORMAT_VERSION_KEYWORDS_VERBOSE="%{?PRINT_KEYWORDS}%{?VERSION_KEYWORDS_VERBOSE}%{!PRINT_ALWAYS}{versionkeywords}%{}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})KEYWORDS:(%{COLOR_RESET})%{?PRINT_ALWAYS}{versionkeywords}%{}\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_KEYWORDS}(%{COLOR_RESET}){}%{!PRINT_ALWAYS}{versionekeywords}%{}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})KEYWORDS*:(%{COLOR_RESET})%{?PRINT_ALWAYS}{!versionekeywords}\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_KEYWORDS_EQUAL}{else}%{}\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_KEYWORDSS}(%{COLOR_RESET}){}%{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the verbose format for IUSE for an available version.
FORMAT_IUSE_VERBOSE="%{?PRINT_IUSE}%{?VERSION_IUSE_VERBOSE}%{!PRINT_ALWAYS}{haveuse}%{}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})IUSE:(%{COLOR_RESET})%{?PRINT_ALWAYS}{haveuse}%{}\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_VERSION_IUSE}(%{COLOR_RESET}){}%{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the verbose format for DEPs for an available version.
FORMAT_DEPS_VERBOSE="%{?DEP}%{?VERSION_DEPS_VERBOSE}%{!PRINT_ALWAYS}{havedepend}%{}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})DEPEND:(%{COLOR_RESET})%{?PRINT_ALWAYS}{depend}%{}\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_DEPEND_VERBOSE}{}%{!PRINT_ALWAYS}{haverdepend}%{}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})RDEPEND:(%{COLOR_RESET})%{?PRINT_ALWAYS}{haverdepend}%{}\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_RDEPEND_VERBOSE}{}%{!PRINT_ALWAYS}{havepdepend}%{}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})PDEPEND:(%{COLOR_RESET})%{?PRINT_ALWAYS}{havepdepend}%{}\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_PDEPEND_VERBOSE}{}%{!PRINT_ALWAYS}{havehdepend}%{}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})HDEPEND:(%{COLOR_RESET})%{?PRINT_ALWAYS}{havehdepend}%{}\\C<%{COLUMN_AVAILABLE_CONTENT}>%{FORMAT_HDEPEND_VERBOSE}{}%{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the verbose format for mask reasons for an available version.
FORMAT_MASKREASONS_VERBOSE="%{?PRINT_MASKREASONS}%{?MASKREASONS_VERBOSE}%{!PRINT_ALWAYS}{havemaskreasons}%{}%{FORMAT_VER_LINESKIP}(%{COLOR_AVAILABLE_TITLE})Mask:(%{COLOR_RESET})%{?PRINT_ALWAYS}{havemaskreasons}%{}%{?WIDETERM}\\C<%{COLUMN_AVAILABLE_CONTENT}>%{else}%{FORMAT_NEWLINE}%{}(%{COLOR_MASKREASONS})<maskreasons*>(%{COLOR_RESET}){}%{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines normal data appended to available versions with --versionlines
FORMAT_VERSION_APPENDIX_NORMAL="%{FORMAT_VERSION_KEYWORDS_NORMAL}%{FORMAT_IUSE_NORMAL}%{FORMAT_DEPS_NORMAL}%{FORMAT_MASKREASONS_NORMAL}"

# STRING
# This variable is only used for delayed substitution.
# It defines verbose data appended to available versions with --versionlines
FORMAT_VERSION_APPENDIX_VERBOSE="%{FORMAT_VERSION_KEYWORDS_VERBOSE}%{FORMAT_IUSE_VERBOSE}%{FORMAT_DEPS_VERBOSE}%{FORMAT_MASKREASONS_VERBOSE}"

# STRING
# This variable is only used for delayed substitution.
# It defines the data appended to available versions with --versionlines
FORMAT_VERSION_APPENDIX="{\$modus=verbose}%{FORMAT_VERSION_APPENDIX_VERBOSE}{else}%{FORMAT_VERSION_APPENDIX_NORMAL}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines lineskip + slot if slotsorted versionlines are used.
FORMAT_SLOTLINESKIP_VERSIONLINES="%{FORMAT_NEWLINE}\\C<%{COLUMN_SLOT}>%{FORMAT_SLOT}"

# STRING
# This variable is only used for delayed substitution.
# It defines lineskip + slot if slotsort but no versionlines are used.
FORMAT_SLOTLINESKIP="%{FORMAT_NEWLINE}\\C<%{COLUMN_SLOT}>%{FORMAT_SLOT} \\C<%{COLUMN_SLOT_AFTER}>"

# STRING
# This variable is only used for delayed substitution.
# It defines lineskip + stability if lineskip is used.
FORMAT_VERSLINESKIP="%{FORMAT_NEWLINE}\\C<%{COLUMN_STABILITY}>%{FORMAT_STABILITY} \\C<%{COLUMN_STABILITY_AFTER}>"

# STRING
# This variable is only used for delayed substitution.
# It defines the lineskip for installed versions.
FORMAT_INST_LINESKIP="%{FORMAT_NEWLINE}\\C<%{COLUMN_INST_TITLE}>"

# STRING
# This variable is used as a version formatter.
# It defines the format for a version; versionsorted with versionlines.
VSORTL="%{FORMAT_VERSLINESKIP}{*sep=\\ \\C<%{COLUMN_SLOT_APPEND}>}%{PVERSIONS_VERBOSE}{!*sep}%{FORMAT_VERSION_APPENDIX}{last}{*sorted=version}{}"

# STRING
# This variable is used as a version formatter.
# It defines the format for a version; versionsorted without versionlines.
VSORT="%{AVERSIONS_VERBOSE}{last}{*sorted=version}{else} {}"

# STRING
# This variable is used as a version formatter.
# It defines the format for a version; slotsorted with versionlines.
SSORTL="{slotfirst}%{FORMAT_SLOTLINESKIP_VERSIONLINES}{}%{FORMAT_VERSLINESKIP}{*sep=\\ \\C<%{COLUMN_VER_APPEND}>}%{PVERSIONO_VERBOSE}{!*sep}%{FORMAT_VERSION_APPENDIX}{last}{*sorted=slot}{}"

# STRING
# This variable is used as a version formatter.
# It defines the format for a version; slotsorted without versionlines.
SSORT="{slotfirst}{oneslot}%{FORMAT_SLOT} {else}%{FORMAT_SLOTLINESKIP}{}{else} {}%{AVERSIONO_VERBOSE}{last}{*sorted=slot}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for collected IUSE data in one line.
FORMAT_COLL="%{FORMAT_BEFORE_COLL}%{FORMAT_COLLIUSE}%{FORMAT_AFTER_COLL}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for collected IUSE data in a separate line.
FORMAT_COLL_SEP="%{FORMAT_BEFORE_COLL_SEP}%{FORMAT_COLLIUSE}%{FORMAT_AFTER_COLL_SEP}"

# STRING
# This variable is only used for delayed substitution.
# It is the format for package IUSE in single-line verbose mode.
FORMAT_COLL_VERBOSE="%{!PRINT_ALWAYS}{havecolliuse}%{}%{FORMAT_NEWLINE}     (%{COLOR_TITLE})IUSE \\(all versions\\):(%{COLOR_RESET})%{?PRINT_ALWAYS}{havecolliuse}%{} %{FORMAT_COLLIUSE}(%{COLOR_RESET}){}"

# STRING
# This variable is only used for delayed substitution.
# It is the format for package IUSE in multiline verbose mode.
FORMAT_COLL_VERBOSE_SEP="%{FORMAT_COLL_VERBOSE}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for collected IUSE data.
FORMAT_COLL_IUSE="%{?PRINT_IUSE}{\$modus=verbose}{\$sorted=version}%{FORMAT_COLL_VERBOSE}{else}%{FORMAT_COLL_VERBOSE_SEP}{}{else}{havecolliuse}{\$sorted=version}%{FORMAT_COLL}{else}%{FORMAT_COLL_SEP}{}{}{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for collected IUSE data with --versionlines.
FORMAT_COLL_IUSE_LIST="%{?PRINT_IUSE}%{!VERSION_IUSE_NORMAL}{\$modus=normal}{havecolliuse}%{FORMAT_COLL_SEP}{}%{?VERSION_IUSE_VERBOSE}{}%{else}{else}%{}%{else}%{!VERSION_IUSE_VERBOSE}{\$modus=verbose}%{}%{}%{!VERSION_IUSE_VERBOSE}%{FORMAT_COLL_VERBOSE_SEP}{}%{}%{}"

# STRING
# This variable is used as a version formatter.
# It defines the normal format of installed versions.
INORMAL="%{IVERSIONS_VERBOSE}(%{COLOR_DATE})\\(<date:FORMAT_INSTALLATION_DATE>\\)(%{COLOR_RESET})%{FORMAT_INST_USEFLAGS}{!last}{versionlines}%{FORMAT_INST_LINESKIP}{else} {}{}"

# STRING
# This variable is used as a version formatter.
# It defines the compact format of installed versions.
ICOMPACT="%{IVERSIONS_COMPACT}(%{COLOR_NORMAL})@(%{COLOR_DATE})<date:FORMAT_SHORT_INSTALLATION_DATE>(%{COLOR_RESET}){!last} {}"

# STRING
# This variable is used as a version formatter.
# It defines the verbose format of installed versions.
IVERBOSE="(%{COLOR_INST_TITLE})Version:(%{COLOR_RESET})\\C<%{COLUMN_INST_CONTENT}>%{IVERSIONS_VERBOSE}(%{COLOR_RESET})%{FORMAT_INST_LINESKIP}(%{COLOR_INST_TITLE})Date:(%{COLOR_RESET})\\C<%{COLUMN_INST_CONTENT}>(%{COLOR_DATE})<date:FORMAT_INSTALLATION_DATE>(%{COLOR_RESET}){haveuse}%{FORMAT_INST_LINESKIP}(%{COLOR_INST_TITLE})USE:(%{COLOR_RESET})\\C<%{COLUMN_INST_CONTENT}>%{?EIX_USE_EXPAND}<use*>%{else}<use>%{}{}%{?DEP}{havedepend}%{FORMAT_INST_LINESKIP}(%{COLOR_INST_TITLE})DEPEND:(%{COLOR_RESET})\\C<%{COLUMN_INST_CONTENT}>%{FORMAT_DEPEND_VERBOSE}{}{haverdepend}%{FORMAT_INST_LINESKIP}(%{COLOR_INST_TITLE})RDEPEND:(%{COLOR_RESET})\\C<%{COLUMN_INST_CONTENT}>%{FORMAT_RDEPEND_VERBOSE}{}{havepdepend}%{FORMAT_INST_LINESKIP}(%{COLOR_INST_TITLE})PDEPEND:(%{COLOR_RESET})\\C<%{COLUMN_INST_CONTENT}>%{FORMAT_PDEPEND_VERBOSE}{}{havehdepend}%{FORMAT_INST_LINESKIP}(%{COLOR_INST_TITLE})HDEPEND:(%{COLOR_RESET})\\C<%{COLUMN_INST_CONTENT}>%{FORMAT_HDEPEND_VERBOSE}{}%{}{!last}%{FORMAT_INST_LINESKIP}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the verbose format for printing available versions.
FORMAT_AVAILABLEVERSIONS="{versionlines}{slotsorted}<availableversions:VSORTL:SSORTL>{else}<availableversions:VSORTL>{}%{FORMAT_COLL_IUSE_LIST}{else}{slotsorted}<availableversions:VSORT:SSORT>{else}<availableversions:VSORT>{}%{FORMAT_COLL_IUSE}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for useflags in installed versions.
FORMAT_INST_USEFLAGS="{haveuse}(%{COLOR_NORMAL})\\(%{?EIX_USE_EXPAND}<use*>%{else}<use>%{}(%{COLOR_NORMAL})\\)(%{COLOR_NORMAL_END}){}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format used for printing installed versions normally.
INSTALLEDVERSIONS_NORMAL="<installedversions:INORMAL>"

# STRING
# This variable is only used for delayed substitution.
# It defines the format used for printing installed versions compactly.
INSTALLEDVERSIONS_COMPACT="<installedversions:ICOMPACT>"

# STRING
# This variable is only used for delayed substitution.
# It defines the format used for printing installed versions verbosely.
INSTALLEDVERSIONS_VERBOSE="<installedversions:IVERBOSE>"

# STRING
# This variable is only used for delayed substitution.
# It defines the format used for printing installed versions.
INSTALLEDVERSIONS="{\$modus=verbose}%{INSTALLEDVERSIONS_VERBOSE}{else}%{INSTALLEDVERSIONS_NORMAL}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for a line with installed versions.
FORMATLINE_INSTALLEDVERSIONS="%{!PRINT_ALWAYS}{installed}%{}\\C<%{COLUMN_TITLE}>(%{COLOR_TITLE})Installed versions:(%{COLOR_RESET})%{?PRINT_ALWAYS}{installed}%{}\\C<%{COLUMN_CONTENT}>%{INSTALLEDVERSIONS}%{?PRINT_ALWAYS}{else}\\C<%{COLUMN_CONTENT}>None{}%{FORMAT_NEWLINE}%{else}%{FORMAT_NEWLINE}{}%{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for eix-diff for installed versions.
DIFF_FORMATLINE_INSTALLEDVERSIONS="{installed}%{INSTALLEDVERSIONS_COMPACT}(%{COLOR_NORMAL});(%{COLOR_NORMAL_END}) {}"

# STRING
# This variable is only used for delayed substitution.
# It prints a newline at the end of a package.
FORMAT_FINISH="()\\n"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for the printing the package name.
FORMAT_NAME="{system}(%{COLOR_CATEGORY_SYSTEM}){else}{world}(%{COLOR_CATEGORY_WORLD}){else}{world_sets}(%{COLOR_CATEGORY_WORLD_SETS}){else}(%{COLOR_CATEGORY}){}{}{}<category>(%{COLOR_NORMAL})/{marked}(%{COLOR_MARKED_NAME}){else}{world}(%{COLOR_WORLD}){else}{world_sets}(%{COLOR_WORLD_SETS}){else}(%{COLOR_NAME}){}{}{}<name>(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for the normal header symbols.
FORMAT_HEADER="{installed}(%{COLOR_NORMAL})[{!*updn}{upgrade}{*updn}%{TAG_UPGRADE}{}{downgrade}{*updn}%{TAG_DOWNGRADE}{}{!\$updn}%{TAG_INSTALLED}{}(%{COLOR_NORMAL})](%{COLOR_NORMAL_END}){else}(%{COLOR_UNINST_TAG})%{STRING_PLAIN_UNINSTALLED}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for the verbose header symbols.
FORMAT_HEADER_VERBOSE="{installed}(%{COLOR_INST_TAG})%{STRING_PLAIN_INSTALLED}{else}(%{COLOR_UNINST_TAG})%{STRING_PLAIN_UNINSTALLED}{}(%{COLOR_RESET})"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for the compact header symbols.
FORMAT_HEADER_COMPACT="(%{COLOR_NORMAL})[{installed}{upgrade}{*upgrade}%{TAG_UPGRADE}{else}{!*upgrade}{}{downgrade}%{TAG_DOWNGRADE}{else}{!\$upgrade}%{TAG_INSTALLED}{}{}{else}%{TAG_UNINSTALLED}{}(%{COLOR_NORMAL})](%{COLOR_NORMAL_END})"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for the diff-new header symbols.
DIFF_FORMAT_HEADER_NEW="(%{COLOR_NORMAL})[{havebest}%{TAG_STABILIZE}{}{upgrade}{*upgrade}%{TAG_UPGRADE}{else}{!*upgrade}{}{downgrade}%{TAG_DOWNGRADE}{else}{!\$upgrade}%{TAG_NEW}{}{}(%{COLOR_NORMAL})](%{COLOR_NORMAL_END})\\C<%{DIFF_COLUMNA}>(%{DIFF_COLOR_NEW})%{DIFF_STRING_NEW}(%{COLOR_RESET})\\C<%{DIFF_COLUMNB}>"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for the diff-delete header symbols.
DIFF_FORMAT_HEADER_DELETE="{installed}(%{COLOR_NORMAL})[(%{COLOR_DOWNGRADE})%{CHAR_DOWNGRADE}(%{COLOR_NORMAL})](%{COLOR_NORMAL_END}){}\\C<%{DIFF_COLUMNA}>(%{DIFF_COLOR_DELETE})%{DIFF_STRING_DELETE}(%{COLOR_RESET})\\C<%{DIFF_COLUMNB}>"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for the diff-changed header symbols.
DIFF_FORMAT_HEADER_CHANGED="(%{COLOR_NORMAL})[{havebest}{!oldhavebest}%{TAG_STABILIZE}{}{}{!*better}{!*worse}{upgrade}%{TAG_UPGRADE}{else}{better}{*better}{}{}{downgrade}%{TAG_DOWNGRADE}{else}{worse}{*worse}{}{}{\$better}%{TAG_BETTER}{}{\$worse}%{TAG_WORSE}{}(%{COLOR_NORMAL})](%{COLOR_NORMAL_END})\\C<%{DIFF_COLUMNA}>(%{DIFF_COLOR_CHANGED})%{DIFF_STRING_CHANGED}(%{COLOR_RESET})\\C<%{DIFF_COLUMNB}>"

# STRING
# This variable is only used for delayed substitution.
# It defines the compact format for the best version(s).
FORMAT_BEST_COMPACT="{havebest}<bestversion:AVERSIONS_COMPACT>{else}{havebest*}<bestversion*:AVERSIONS_COMPACT>{else}%{?NOBEST_COMPACT}%{FORMAT_NOBEST}%{else}<availableversions:VSORT>%{}{}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the compact format for the best version(s) in case of changes.
FORMAT_BEST_CHANGE="{havebest}<bestslotversions:VSORT>{else}{havebest*}<bestslotversions*:VSORT>{else}%{?NOBEST_CHANGE}%{FORMAT_NOBEST_CHANGE}%{else}<availableversions:VSORT>%{}{}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the eix-diff format for the best version(s).
DIFF_FORMAT_BEST="{havebest}<bestversion:AVERSIONS_COMPACT>{else}{havebest*}<bestversion*:AVERSIONS_COMPACT>{else}%{?DIFF_NOBEST}%{FORMAT_NOBEST}%{else}<availableversions:VSORT>%{}{}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the eix-diff format for the best version(s) in case of changes.
DIFF_FORMAT_BEST_CHANGE="{havebest}<bestslotversions:VSORT>{else}{havebest*}<bestslotversions*:VSORT>{else}%{?DIFF_NOBEST_CHANGE}%{FORMAT_NOBEST_CHANGE}%{else}<availableversions:VSORT>%{}{}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the eix-diff format for the old best versions in case of changes.
DIFF_FORMAT_OLDBEST_CHANGE="{oldhavebest}<oldbestslotversions:VSORT>{else}{oldhavebest*}<oldbestslotversions*:VSORT>{else}%{?DIFF_NOBEST_CHANGE}%{FORMAT_NOBEST_CHANGE}%{else}<oldavailableversions:VSORT>%{}{}{}"

# STRING
# This variable is only used for delayed substitution.
# It defines the eix-diff format for changed versions.
DIFF_FORMAT_CHANGED_VERSIONS="%{?DIFF_PRINT_INSTALLED}%{DIFF_FORMATLINE_INSTALLEDVERSIONS}%{}%{DIFF_FORMAT_OLDBEST_CHANGE} (%{COLOR_NORMAL})->(%{COLOR_NORMAL_END}) %{DIFF_FORMAT_BEST_CHANGE}"

# STRING
# This variable is only used for delayed substitution.
# It defines the format for the printing the optional overlay key.
FORMAT_OVERLAYKEY="{overlaykey} <overlaykey>{}"

